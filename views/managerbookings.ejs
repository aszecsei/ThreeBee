<html>
<head>
    <% title = "Flights"; %>
    <% include ./partials/head.ejs %>
</head>
<body class="container">
<header>
    <% include ./partials/header.ejs %>
</header>

<main>
    <%for(var k =0; k<result.length; k++){%>
    <div class="jumbotron">
        <table class="table">
            <thread>
                <th>#</th>
                <th>Booking ID</th>
                <th>Flight ID</th>
                <th>Booking Type</th>
            </thread>
            <tbody>
            <%var j = 1; for(var i = 0; i < result[k].length; i++){ %>
            <tr>
                <th scope="row"><%=j%></th>
                <td><%=result[k][i].bookingID%></td>
                <td><%=result[k][i].flightID%></td>
                <td><%=result[k][i].bookingType%></td>
                <td><a href= planes/<%= result[i].bookingID%>>Delete</a> </td>
            </tr>
            <%j++;}%>
        </table>
    </div>
    <%}%>

</main>

<footer>
    <% include ./partials/footer.ejs %>
</footer>

</body>
</html>